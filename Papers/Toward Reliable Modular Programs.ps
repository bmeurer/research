/inch { 72 mul } def

/dot		% params: center_x center_y  ==>
{ newpath
  1.5 0 360 arc
  fill
} def

/circledot	% params: center_x center_y  ==>
{ 2 copy
  dot
  3 0 360 arc
  stroke
} def

/topoints	% params: [x y}  ==>  x_in y_in
{ aload pop
  inch exch
  inch exch
} def

/Dot { topoints dot } def
/Circledot { topoints circledot } def
/Moveto { topoints moveto } def
/Lineto { topoints lineto } def

/shade		% params:  u_x u_y ll_x ll_y lr_x lr_y  ==>
{ gsave
    newpath
    3 index 3 index	% u_x u_y ll_x ll_y lr_x lr_y  ll_x ll_y
    moveto		% u_x u_y ll_x ll_y lr_x lr_y

    exch 3 index sub	% u_x u_y ll_x ll_y lr_y lr_x-ll_x
    exch 2 index sub	% u_x u_y ll_x ll_y lr_x-ll_x lr_y-ll_y
			% = ... dx dy

    1 index 0.6 mul	% ... dx dy  dx*0.6
    1 index 1.3 mul	% ... dx dy  dx*0.6 y*1.3

    3 index 0.4 mul
    3 index -0.4 mul	% ... dx dy  dx*0.6 dy*1.3 dx*0.4 -dy*0.4

    5 index 5 index	% ... dx dy  dx*0.6 dy*1.3 dx*0.4 -dy*0.4 dx dy
    rcurveto		% ... dx dy

    pop pop pop pop	% u_x u_y
    lineto		%
    closepath

    0.9 setgray
    fill
  grestore
} def


