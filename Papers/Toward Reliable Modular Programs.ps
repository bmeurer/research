% forestgraph.ps
% K.R.M. Leino

/inch { 72 mul } def

/vertex		% params: center_x center_y  ==>
{ newpath
  1.5 0 360 arc
  fill
} def

/topoints	% params: [x y]  ==>  x_inches y_inches
{ aload pop
  inch exch
  inch exch
} def

/emptySpace 3 def

/simpleedge	% params: length
{ 0.5 setlinewidth
  emptySpace 0 moveto
  dup emptySpace sub 0 lineto
  stroke
  gsave
    0.5 mul 0 translate
    -2 2 moveto
    2 0 lineto
    -2 -2 lineto
    stroke
  grestore
} def

/angle		% params: dx dy  ==>  angle
{ exch atan
} def

/distance	% params: dx dy  ==>  sqrt(dx^2 + dy^2)
{ dup mul
  exch
  dup mul
  add
  sqrt
} def

/vector		% params: dx dy  ==>
{ 2 copy angle	% dx dy angle
  rotate	% dx dy
  distance	% D
  simpleedge	%
} def

/to_dxdy	% params: x0 y0 x1 y1  ==>  x1-x0 y1-y0
{ 3 -1 roll	% x0 x1 y1 y0
  sub		% x0 x1 y1-y0
  3 1 roll	% y1-y0 x0 x1
  exch sub	% y1-y0 x1-x0
  exch		% x1-x0 y1-y0
} def

/edge		% params: source_x source_y sink_x sink_y  ==>
{ gsave
    3 index 3 index translate
    to_dxdy vector
  grestore
} def

/Vertex { topoints vertex } def
/Edge { topoints 3 -1 roll topoints 4 2 roll edge } def


/ptA [0.2 0.8] def
