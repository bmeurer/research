%%BeginProcSet: statespace.ps
%  statespace.ps
%  K.R.M. Leino
%  20 Oct 1994


/inch { 72 mul } def
/zheight 0.7 inch def

/fourpoints	% params: proc0 proc123 x y
{ 2 copy		% proc0 proc123 x y x y
  6 -1 roll		% proc123 x y x y proc0
  exec			% proc123 x y

  exch			% proc123 y' x
  1 inch add exch	% proc123 x' y'
  2 copy		% proc123 x' y' x' y'
  4 index		% proc123 x' y' x' y' proc123
  exec			% proc123 x' y'

  0.5 inch add exch	% proc123 y'' x'
  0.5 inch add exch	% proc123 x'' y''
  2 copy		% proc123 x'' y'' x'' y''
  4 index exec		% proc123 x'' y''

  exch			% proc123 y''' x''
  1 inch sub exch	% proc123 x''' y'''
  3 -1 roll		% x''' y''' proc123
  exec			%
} def

/Plane		% params: lower_left_x lower_left_y
{ newpath
  {moveto} {lineto}	% x y proc0 proc123
  4 2 roll		% proc0 proc123 x y
  fourpoints
  closepath
  gsave 0.9 setgray fill grestore
  stroke
} def

/showBottom
{ gsave
    0.5 setlinewidth
    6 0 rlineto
    -3 0 rmoveto
    0 8 rlineto
    stroke
  grestore
} def

/showTop
{ gsave
    0.5 setlinewidth
    3 0 rmoveto
    0 8 rlineto
    -3 0 rmoveto
    6 0 rlineto
    stroke
  grestore
} def


/lowerplane
{ 0 0 Plane
  /Times-Italic findfont 12 scalefont setfont
  0.5 inch -12 moveto (a) show
  1.32 inch 0.15 inch moveto (b) show
} def


/upperplane
{ gsave
    [2 2] 0 setdash
    {newpath moveto 0 zheight rlineto stroke} dup
    0 0 fourpoints
  grestore
  0 zheight Plane
  /Times-Italic findfont 12 scalefont setfont
  -15 zheight 2 div -4 add moveto (oc) show
  -12 -5 moveto showBottom
  -12 zheight -5 add moveto showTop
} def


/shiftAndPrintHeader
{ /Times-Roman findfont 12 scalefont setfont
  -30 7 moveto (Normal) show
  -30 -5 moveto (states) show
  -30 3 zheight 7 add add moveto (Exceptional) show
  -30 3 zheight -5 add add moveto (states) show
  0.3 inch 0 translate
} def


/traceDot
{ 2 0 360 arc fill
} def

/sampleTraceCoords
[ [ 40 25 ]
  [ 60 23 ]
  [ 52 30 ]
  [ 65 70 ]
  [ 95 32 ]
  [ 80 27 ]
]
def

/sampleTrace
{ newpath
  35 10 moveto
  sampleTraceCoords
  { aload pop lineto
  } forall
  currentpoint	% last_x last_y
  stroke
  moveto	%
  80 15 lineto
  stroke
  77 20 moveto	% arrow
  80 15 lineto
  83 20 lineto
  stroke
} def

/dotsTrace
{ newpath
  35 10 traceDot
  sampleTraceCoords
  { aload pop traceDot
  } forall
} def


/sampleRaiseTrace
{ newpath
  50 15 moveto
  50 15 zheight add lineto
  stroke
  47 15 zheight 2 div add moveto
  3 5 rlineto
  3 -5 rlineto
  stroke
} def

/dotsRaiseTrace
{ newpath
  50 15 traceDot
  50 15 zheight add traceDot
} def


% 1 inch 5 inch translate
% lowerplane

% 2 inch 0 translate
% lowerplane upperplane

