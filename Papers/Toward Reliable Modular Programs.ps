/tostring
{ 1 string dup 0
  4 -1 roll 
  put		% these three lines convert the char code to a 1-char string
} def


/chNstartX 0 def
/chNstartY 0 def

/ChNumber	% params:	ch_num_str
{ /Civitype findfont 250 scalefont setfont
  dup dup
  stringwidth neg exch neg exch rmoveto
  currentpoint		% cx cy
  /chNstartY exch def	% cx
  /chNstartX exch def	%
  ChCh
  gsave
    true charpath
    gsave
      1 setgray fill
    grestore
    clip
    -10 0.6 10
    {			% i
      dup -5		% i i -5
      chNstartX add	% i i cx-5
      exch		% i cx-5 i
      8 mul		% i cx-5 8*i
      chNstartY add	% i cx-5 8*i+cy
      moveto		% i

      dup		% i i
      -4 div		% i -i/4
      10 add		% i -i/4+10
      exch		% -i/4+10 i
      4 mul		% -i/4+10 4*i
      50 add		% -i/4+10 4*i+50
      lineto		%

      stroke		%
    } for
  grestore
  gsave
    false charpath
    2.5 setlinewidth
    stroke
  grestore
} def

/ChCh
{ gsave
    /Civitype findfont 24 scalefont setfont
    -17 0 rmoveto
    gsave
      0.96 setgray
      -10 -10 rmoveto
      0 105 rlineto 10 95 lineto 10 -10 lineto closepath
      fill
    grestore
    10 70 rmoveto
    (Chapter) rshow
  grestore
} def

/rshow		% params:	str
{ dup
  stringwidth neg exch neg exch rmoveto
  gsave show grestore
} def

/ChNumberSimple	% params:	str
{ /Civitype findfont 120 scalefont setfont
  rshow
  /Civitype findfont 24 scalefont setfont
  -10 30 rmoveto
  (Chapter) rshow
} def

/Chapter	% params:	ch_num_str
{ -10 0 translate
  0 0 moveto
  % ChNumber
  ChNumberSimple
} def


/PartLine
