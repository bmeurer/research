} def


/topoints	% params:  [xy] ==> x_in y_in
{ aload pop			% x y
  inch exch			% y_in x
  inch exch			% x_in y_in
} def


/Part		% params:  [xy] str ==>
{ dup stringwidth pop		% [xy] str dx'str
  (Part ) stringwidth pop add	% [xy] str dx

  gsave				% [xy] str dx
    2 index topoints translate	% [xy] str dx
    dup 2 div dup neg		% [xy] str dx dx/2 -dx/2
    -2 moveto -2 lineto		% [xy] str dx
    stroke
  grestore			% [xy] str dx

  gsave
    3 -1 roll			% str dx [xy]
    topoints			% str dx x y
    3 1 roll exch		% str y x dx
    -2 div add exch		% str x-dx/2 y
    moveto			% str
    (Part ) show show		%
  grestore
} def


/Chapter	% params:  [xy] str ==>
{ dup stringwidth pop		% [xy] str dx'str
  (Ch. ) stringwidth pop add	% [xy] str dx

  gsave				% [xy] str dx
    2 index topoints translate	% [xy] str dx
    WW				% [xy] str dx w
    newpath
    dup -2 div Yoffset moveto
    0 HH rlineto
    0 rlineto			% [xy] str dx
    0 HH neg rlineto
    closepath
    stroke
  grestore			% [xy] str dx

  gsave				% [xy] str dx
    3 -1 roll			% str dx [xy]
    topoints			% str dx x y
    3 1 roll exch		% str y x dx
    -2 div add exch		% str x-dx/2 y
    moveto			% str
    (Ch. ) show show		%
  grestore
} def


/emptySpace 3 def

/simpleedge	% params: length
{ emptySpace 0 moveto			% length
  emptySpace sub 0			% sink_x sink_y
  2 copy lineto				% sink_x sink_y
  stroke
  gsave
    translate				%
    -4 2 moveto
    0 0 lineto
    -4 -2 lineto
    stroke
  grestore
} def

/angle		% params: dx dy  ==>  angle
{ exch atan
} def

/distance	% params: dx dy  ==>  sqrt(dx^2 + dy^2)
{ dup mul
  exch
  dup mul
  add
  sqrt
} def

/vector		% params: dx dy  ==>
{ 2 copy angle	% dx dy angle
  rotate	% dx dy
  distance	% D
  simpleedge	%
} def

/to_dxdy	% params: x0 y0 x1 y1  ==>  x1-x0 y1-y0
{ 3 -1 roll	% x0 x1 y1 y0
  sub		% x0 x1 y1-y0
  3 1 roll	% y1-y0 x0 x1
  exch sub	% y1-y0 x1-x0
  exch		% x1-x0 y1-y0
} def

/Arrow		% params: source_x source_y sink_x sink_y  ==>
{ gsave
    3 index 3 index translate
    to_dxdy vector
  grestore
